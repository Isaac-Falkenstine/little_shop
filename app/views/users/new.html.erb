<%= form_for @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :zip %>
  <%= f.number_field :zip %>

  <%= f.label :states %>
  <%= f.text_field :states %>

  <%= f.label :email_address %>
  <%= f.text_field :email_address %>

  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit %>
<% end %>
