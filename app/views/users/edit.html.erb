<h1>Edit your info here:</h1>

<%= form_for @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :zip %>
  <%= f.number_field :zip %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :email_address %>
  <%= f.text_field :email_address %>

  <%= f.submit %>
<% end %>
